/* recursive algorithm for calulating nth fibinacci number */
funk int int fib(int n, int val)
{

	if(n == 1){
		return 1;
	}

	else{

		val = $factorial(n-1, 0);

		n = val * n;

		return n; 
	}

}

/* this is a working gcd algorthim */

funk int int gcd(int a, int b){

	while(a != b){
		if (a > b) {
			a = a- b;
			}
		else
		{
			b = b - a;
		}
	}

	return a;
}


/* prints the gcd and factorial */

(print(), print()) [gcd(30, 90), fib(2, 0)]; 



